{
    "manifest_version": 3,
    "name": "Refresh",
    "description": "Productivity Extension",
    "version": "1.0",
    "permissions": ["tabs", "activeTab", "scripting", "storage", "webNavigation"],
    "host_permissions": [
      "http://localhost:3000/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_title": "Click on this button"
    }
  }

  